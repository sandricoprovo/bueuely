{
    "name": "app",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "db:start": "cd ./supabase && supabase start",
        "db:stop": "cd ./supabase && supabase stop --backup",
        "db:migration": "cd ./supabase && supabase db diff --schema public -f",
        "db:push": "cd ./supabase && supabase db reset"
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^2.0.3",
        "@tsconfig/svelte": "^4.0.1",
        "svelte": "^3.57.0",
        "svelte-check": "^2.10.3",
        "svelte-routing": "^1.8.9",
        "tslib": "^2.5.0",
        "typescript": "^5.0.2",
        "vite": "^4.3.2"
    },
    "dependencies": {
        "@supabase/supabase-js": "^2.22.0"
    }
}
